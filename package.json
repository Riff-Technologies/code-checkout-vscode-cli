{
  "name": "code-checkout-cli",
  "publisher": "riff-tech",
  "version": "1.0.0",
  "description": "CLI tool for Code Checkout platform integration",
  "main": "dist/index.js",
  "bin": {
    "code-checkout": "./bin/run"
  },
  "scripts": {
    "build": "tsc",
    "build:dev": "NODE_ENV=development npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "dev": "NODE_ENV=development npm run clean && npm run build && npm link",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest",
    "start": "NODE_ENV=development ./bin/run"
  },
  "dependencies": {
    "@oclif/core": "2.11.8",
    "@oclif/plugin-help": "5.2.20",
    "axios": "1.6.2",
    "dotenv": "^16.4.7",
    "inquirer": "8.2.5",
    "open": "8.4.2"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.5",
    "@types/jest": "29.5.5",
    "@types/node": "18.18.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.45.0",
    "jest": "29.5.0",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "code-checkout",
    "dirname": "code-checkout",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
